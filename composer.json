{
    "name": "andres-ml/cakephp-i18n",
    "description": "I18n plugin for CakePHP",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=5.4.16",
        "cakephp/cakephp": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "Aml\\I18n\\": "src/I18n"
        },
        "files": [
            "src/I18n/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Aml\\I18n\\Test\\": "tests"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "I18n\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
