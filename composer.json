{
    "name": "andres-ml/cakephp-i18n",
    "description": "I18n plugin for CakePHP",
    "type": "cakephp-plugin",
    "keywords": [
        "cakephp",
        "i18n"
    ],
    "homepage": "https://github.com/andres-ml/cakephp-i18n",
    "authors": [
        {
            "name":"AndrÃ©s ML",
            "role":"Author",
            "homepage":"https://github.com/andres-ml"
        }
    ],
    "require": {
        "php": ">=5.4.16",
        "cakephp/cakephp": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "Aml\\I18n\\": "src"
        },
        "files": [
            "src/I18n/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Aml\\I18n\\Test\\": "tests"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "I18n\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
